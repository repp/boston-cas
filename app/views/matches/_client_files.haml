- client = match.client
.detail-box--label= link_to 'Files', Config.get(:warehouse_url) + "/window/clients/#{client.remote_id}/files", target: '_blank' 
.detail-box--value
  - if @available_files.any?
    -# Show the most recent version of any matching files
    - @sub_program.file_tags.each do |tag|
      - if file = @available_files.detect{|file| file.tag_id == tag.tag_id}
        .file= tag.name
  - else
    None