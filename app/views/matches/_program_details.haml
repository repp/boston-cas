- if MatchRoutes::Base.more_than_one?
  %h3.detail-box--label Match Route
  .detail-box--value
    = @program.match_route.title
%h3.detail-box--label Funding Source
.detail-box--value
  = @program.funding_source.present? ? @program.funding_source.name : 'None'
%h3.detail-box--label Program
.detail-box--value
  = @program.name
  %em
    = link_to @sub_program.name, edit_program_sub_program_path(@program, @sub_program)
%h3.detail-box--label Service Provider
.detail-box--value= match.opportunity_details.service_provider_name
-#%h3.detail-box--label Sub-Contractor
.detail-box--value
